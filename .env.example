# Database credentials
DATABASE_PASSWORD=your-database-password

# Supabase configuration
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_JWT_SECRET=your-jwt-secret

# OpenAI configuration (for AI-powered features)
OPENAI_API_KEY=your-openai-api-key

# Vite specific variables (exposed to frontend)
VITE_SUPABASE_URL=${SUPABASE_URL}
VITE_SUPABASE_ANON_KEY=${SUPABASE_ANON_KEY}
VITE_APP_NAME="Vite-Project-Name"

# Database connection string (for direct connections)
DATABASE_URL=postgresql://postgres:${DATABASE_PASSWORD}@db.your-project-ref.supabase.co:5432/postgres

# Environment mode (development, test, production)
NODE_ENV=development